echo "enter three numberer"
read num1
read num2
read num3
if [ $num1 -lt $num2 ] && [ $num1 -lt $num3 ]
then
       echo "$num1 is smallest number"
elif [ $num2 -lt $num1 ] && [ $num2 -lt $num3 ]
then
       echo "$num2 is smallest number"
else
       echo "$num3 is smallest number"
fi

output:-
enter three numberer
4
5
6
4 is greatest number
