echo "enter three numberer"
read num1
read num2
read num3
if [ $num1 -gt $num2 ] && [ $num1 -gt $num3 ]
then
       echo "$num1 is greatest number"
elif [ $num2 -gt $num1 ] && [ $num2 -gt $num3 ]
then
       echo "$num2 is gratest number"
else
       echo "$num3 is greatest number"
fi

output:-
enter three numberer
4
5
6
6 is greatest number
